{
  "name": "OriginMarker",
  "version": "1.0",
  "description": "Origin dependent marker",
  "permissions": ["tabs", "bookmarks", "storage"],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "128": "icon.jpg",
    "48": "icon48.jpg",
    "32": "icon32.jpg",
    "16": "icon16.jpg"
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "manifest_version": 3
}
