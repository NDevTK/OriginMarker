{
  "name": "originmarker-formatting",
  "version": "1.0.0",
  "description": "Formatting scripts for OriginMarker",
  "devDependencies": {
    "prettier": "3.2.5"
  },
  "scripts": {
    "format": "prettier . --write --single-quote --no-bracket-spacing --bracket-same-line --trailing-comma none --cache --cache-location=prettier_cache"
  }
}
